{
  "name": "twentyone-backend",
  "version": "1.0.0",
  "main": "app.ts",
  "scripts": {
    "start": "node ./dist/app.js",
    "build": "tsc",
    "postinstall": "npm run build && rm -v -rf src",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "migrate": "knex migrate:latest --knexfile ./knexfile.js",
    "migrate:up": "knex migrate:up --knexfile ./knexfile.js",
    "migrate:down": "knex migrate:down --knexfile ./knexfile.js",
    "migrate:list": "knex migrate:list --knexfile ./knexfile.js"
  },
  "engines": {
    "node": "14.x"
  },
  "author": "",
  "license": "NONE",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.15",
    "objection": "^2.2.6",
    "pg": "^8.5.1",
    "socket.io": "^3.0.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.11",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "sqlite3": "^5.0.1",
    "typescript": "^4.1.3"
  }
}
